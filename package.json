    "url-polyfill": "^1.0.7",
    "walk": "^2.3.9",
    "webpack": "3.10.0",
    "webpack-dev-server": "2.11.1",
    "webpack-merge": "4.1.1"
  },
  "eslintConfig": {
    "extends": "openlayers"
  }
