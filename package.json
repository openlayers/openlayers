{
  "name": "openlayers",
  "version": "3.12.1",
  "description": "Build tools and sources for developing OpenLayers based mapping applications",
  "keywords": [
    "map",
    "mapping",
    "ol3"
  ],
  "homepage": "http://openlayers.org/",
  "scripts": {
    "install": "node tasks/install.js",
    "postinstall": "closure-util update",
    "start": "node tasks/serve.js",
    "pretest": "eslint tasks test src examples",
    "test": "node tasks/test.js",
    "debug-server": "node tasks/serve-lib.js"
  },
  "main": "dist/ol.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/openlayers/ol3.git"
  },
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/openlayers/ol3/issues"
  },
  "browser": "dist/ol.js",
  "style": [
    "css/ol.css"
  ],
  "dependencies": {
    "async": "1.5.2",
    "browserify": "12.0.1",
    "closure-util": "1.10.0",
    "derequire": "2.0.3",
    "fs-extra": "0.26.4",
    "glob": "6.0.3",
    "graceful-fs": "4.1.2",
    "handlebars": "4.0.5",
    "jsdoc": "3.4.0",
    "marked": "0.3.5",
    "metalsmith": "2.1.0",
    "metalsmith-layouts": "1.4.2",
    "nomnom": "1.8.1",
    "pbf": "1.3.5",
    "pixelworks": "1.0.0",
    "rbush": "1.4.2",
    "temp": "0.8.3",
    "vector-tile": "1.2.0",
    "walk": "2.3.9"
  },
  "devDependencies": {
    "clean-css": "3.4.9",
    "coveralls": "2.11.6",
    "debounce": "^1.0.0",
    "eslint": "git://github.com/eslint/eslint.git#4356532469ec1b9d4977aff05a03c3e167922ffa",
    "eslint-config-openlayers": "^2.0.0",
    "expect.js": "0.3.1",
    "gaze": "^0.5.1",
    "istanbul": "0.4.1",
    "jquery": "2.1.4",
    "jshint": "2.8.0",
    "mocha": "2.3.4",
    "mocha-phantomjs-core": "^1.3.0",
    "mustache": "2.2.1",
    "phantomjs": "1.9.19",
    "proj4": "2.3.12",
    "resemblejs": "2.0.1",
    "sinon": "1.17.2",
    "slimerjs-edge": "0.10.0-pre-3",
    "wrench": "1.5.8"
  },
  "eslintConfig": {
    "extends": "openlayers",
    "globals": {
      "ArrayBuffer": false,
      "Uint16Array": false,
      "Uint32Array": false,
      "Uint8Array": false,
      "Uint8ClampedArray": false,
      "ol": false,
      "goog": false,
      "proj4": false
    },
    "rules": {
      "no-constant-condition": 0,
      "no-multiple-empty-lines": [2, {"max": 3, "maxEOF": 1}]
    }
  },
  "ext": [
    "rbush",
    {
      "module": "pbf",
      "browserify": true
    },
    {
      "module": "pixelworks",
      "browserify": true
    },
    {
      "module": "vector-tile",
      "name": "vectortile",
      "browserify": true
    }
  ]
}
