{
    "opts": {
        "recurse": true,
        "template": "config/jsdoc/info"
    },
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "includePattern": "\\.js$"
    },
    "plugins": [
        "config/jsdoc/info/api-plugin",
        "config/jsdoc/info/module-plugin"
    ]
}
